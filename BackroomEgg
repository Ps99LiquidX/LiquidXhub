local OrionLib = loadstring(game:HttpGet('https://raw.githubusercontent.com/shlexware/Orion/main/source'))()

-- Notification
OrionLib:MakeNotification({
    Name = "LiquidX Hubüíß",
    Content = "LiquidX Hubüíß",
    Image = "rbxassetid://4483345998",
    Time = 5
})

-- Main Window
local Window = OrionLib:MakeWindow({
    Name = "LiquidX Hubüíß BACKROOMSüëª",
    HidePremium = false,
    SaveConfig = true,
    ConfigFolder = "Orion"
})

-- Information Tab
local InformationTab = Window:MakeTab({
    Name = "Information",
    Icon = "rbxassetid://16468487927",
    PremiumOnly = false
})

local InformationSection = InformationTab:AddSection({
    Name = "Script Information ‚ùì"
})

InformationTab:AddParagraph("Some Exploits Still In Testing! üõ†Ô∏è", "Some Of The Exploits Contained In This Script Hub Are Still In The Testing Phase, Some Features May Not Work As Intended!")
InformationTab:AddLabel("Script Hub Made By LiquidX Team!üíß")

local message = require(game.ReplicatedStorage.Library.Client.Message)

InformationTab:AddButton({
    Name = "Copy Discord Invite",
    Callback = function()
        setclipboard("https://discord.gg/TZwhqbxFQB")
        message.Error("Discord Invite Link Copied!")
    end
})

-- Settings Tab
local SettingsTab = Window:MakeTab({
    Name = "Backrooms Egg",
    Icon = "rbxassetid://15172353804",
    PremiumOnly = false
})

local SettingsSection = SettingsTab:AddSection({
    Name = "Auto Find 100X Egg Luck Settings ‚öôÔ∏è"
})

SettingsTab:AddToggle({
    Name = "Toggle Find 100X Luck Egg!üçÄ",
    Default = false,
    Callback = function(Value)
        if Value then
            local message = require(game.ReplicatedStorage.Library.Client.Message)
            message.Error("Scanning For 100X Luck Eggs!")
            loadstring(game:HttpGet("https://pastebin.com/raw/LxXNA3Sm"))()
        else
            -- Add code here if you want to do something when the toggle is turned off
        end
    end    
})

SettingsTab:AddParagraph("How To Use Find 100X Luck Egg üçÄ", 'Go To World 1 Or 2 Spawn, then toggle on the "Find 100X Luck Egg!üçÄ". It Will Automatically Look For The 100X Luck Egg!')
SettingsTab:AddParagraph("IMPORTANT!‚ö†Ô∏è", 'Put Script Into AutoExec Folder If You Want It To Server Hop Every Hour!‚åõ')


-- Misc Tab
local MiscTab = Window:MakeTab({
    Name = "Misc Scripts",
    Icon = "rbxassetid://17109789793",
    PremiumOnly = false
})

local MiscSection = MiscTab:AddSection({
    Name = "Unlock FPS üîì"
})

MiscSection:AddToggle({
    Name = "Unlock Roblox FPS üîì",
    Default = false,
    Callback = function(Value)
        if Value then
            local message = require(game.ReplicatedStorage.Library.Client.Message)
            message.Error("Unlocked Roblox FPS!")
        end
    end
})

MiscSection:AddParagraph("How To Unlock Roblox FPS üîì", "Toggle The Unlock Roblox FPS. After Done, It Will Unlock The Roblox Client FPS.")

-- Script Hub Tab
local ScriptHubTab = Window:MakeTab({
    Name = "Script Hub",
    Icon = "rbxassetid://14968191202",
    PremiumOnly = false
})

local ScriptHubSection = ScriptHubTab:AddSection({
    Name = "LiquidX Script Hub ‚öôÔ∏è"

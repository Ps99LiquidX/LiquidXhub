-- Misc Scripts --
local Tab = Window:MakeTab({
    Name = "Misc Exploits",
    Icon = "rbxassetid://17109789793",
    PremiumOnly = false
})

local SettingsSection = Tab:AddSection({
	Name = "Auto Digsite ⛏️"
})

SettingsSection:AddToggle({
    Name = "Toggle Auto Digsite ⛏️",
    Default = false,
    Callback = function(Value)
        if Value then
            local message = require(game.ReplicatedStorage.Library.Client.Message)
            message.Error("Auto Digsite Enabled! Please Wait A Few Moments To Load!")
            spawn(function()
                while wait() do
                    for i,v in pairs(workspace.__THINGS.__INSTANCE_CONTAINER.Active.Digsite.Important.ActiveBlocks:GetChildren()) do
                        if v.Name == "Part" and (v.Position and (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - v.Position).magnitude < 15) then
                            for i = 1, 5 do
                                local args = {
                                    [1] = "Digsite",
                                    [2] = "DigBlock",
                                    [3] = v:GetAttribute('Coord')
                                }
                                game:GetService("ReplicatedStorage"):WaitForChild("Network"):WaitForChild("Instancing_FireCustomFromClient"):FireServer(unpack(args))
                            end
                            wait(0.1)
                            break
                        end
                    end
                end
            end)
        end
    end
})

SettingsSection:AddParagraph("How To Use Auto Digsite ⛏️","Go To The Digsite Minigame, And Toggle On The Auto Digsite. It Will Start To Automatically Break Blocks!")

local SettingsSection = Tab:AddSection({
Name = "Auto Pickup 💧"  

})

SettingsSection:AddToggle({
    Name = "Toggle Auto Pickup 💧",
    Default = false,
    Callback = function(Value)
        if Value then
            local message = require(game.ReplicatedStorage.Library.Client.Message)
            message.Error("Auto Pickup Has Been Enabled! Please Wait A Few Moments!")

            local OrbRemote = game:GetService("ReplicatedStorage"):WaitForChild("Network"):WaitForChild("Orbs: Collect")
            local LootbagRemote = game:GetService("ReplicatedStorage"):WaitForChild("Network"):WaitForChild("Lootbags_Claim")
            local OrbFolder = workspace:WaitForChild("__THINGS"):WaitForChild("Orbs")
            local LootbagFolder = workspace:WaitForChild("__THINGS"):WaitForChild("Lootbags")
            local HRP = game.Players.LocalPlayer.Character.HumanoidRootPart

            local function HandleOrb(Orb)
                task.wait(1.5)
                Orb:PivotTo(HRP.CFrame)
                OrbRemote:FireServer({Orb.Name})
                task.wait()
                Orb:Destroy()
            end

            local function HandleLootbag(Lootbag)
                task.wait(4)
                Lootbag:PivotTo(HRP.CFrame)
                LootbagRemote:FireServer({Lootbag.Name})
                task.wait()
                Lootbag:Destroy()
            end

            OrbFolder.ChildAdded:Connect(HandleOrb)
            LootbagFolder.ChildAdded:Connect(HandleLootbag)

            -- Store connections to disconnect later if needed
            local connections = {
                OrbConnection = OrbFolder.ChildAdded:Connect(HandleOrb),
                LootbagConnection = LootbagFolder.ChildAdded:Connect(HandleLootbag)
            }
        else
            -- Disconnect connections if auto pickup is disabled
            if connections then
                connections.OrbConnection:Disconnect()
                connections.LootbagConnection:Disconnect()
            end
        end
    end
})

SettingsSection:AddParagraph("How To Use Auto Pickup 💧","Toggle The Auto Pickup. After Done, It Will Automatically Pickup All The Orbs, Coins, And Lootbags.")

local SettingsSection = Tab:AddSection({
	Name = "Unlock FPS 🔓"
})

SettingsSection:AddToggle({
    Name = "Unlock Roblox FPS 🔓",
    Default = false,
    Callback = function(Value)
        if Value then
            local message = require(game.ReplicatedStorage.Library.Client.Message)
            message.Error("Unlocked Roblox FPS!")
        end
    end
})

SettingsSection:AddParagraph("How To Unlock Roblox FPS 🔓","Toggle The Unlock Roblox FPS. After Done, It Will Unlock The Roblox Client FPS.")
 

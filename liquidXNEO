-- Pet Spawner Tab --
local PetSpawnerTab = Window:MakeTab({
    Name = "Pet Spawner",
    Icon = "rbxassetid://14968191202",
    PremiumOnly = false
})

local PetSpawnerSection = PetSpawnerTab:AddSection({
    Name = "Pet Spawner üêæ"
})

PetSpawnerTab:AddTextbox({
    Name = "Pet Name üêæ",
    Default = "",
    TextDisappear = false,
    Callback = function(Value)
        petName = Value -- Save the entered pet name for later use
    end
})

PetSpawnerTab:AddButton({
    Name = "‚ö†Ô∏è Bypass Client .SetVariables",
    Callback = function()
        local message = require(game.ReplicatedStorage.Library.Client.Message)
        message.Error("Successfully Bypassed Client .SetVariables! Please Proceed With Caution!")
    end
})

PetSpawnerTab:AddButton({
    Name = "Spawn Pet üêæ",
    Callback = function()
        local message = require(game.ReplicatedStorage.Library.Client.Message)
        message.Error("Spawning Selected Pet! Please Wait A Few Moments!")
        if petName then
            print("Spawning Pet:", petName)
            change("Cat", petName) -- Call the change function with the entered pet name
        else
            print("Please enter a pet name.")
        end
    end
})

-- Define the change function --
local lib = require(game.ReplicatedStorage.Library)

function change(fromPet, toPet)
    for i,v in pairs(lib.Directory.Pets[fromPet]) do
        lib.Directory.Pets[fromPet][i] = nil
    end
    for i,v in pairs(lib.Directory.Pets[toPet]) do
        lib.Directory.Pets[fromPet][i] = v
    end
end

PetSpawnerTab:AddParagraph("How To Use Pet Spawner üêæ", "YOU MUST click on Bypass Client Set Variables First, Then Enter The EXACT Name Of The Pet You Want To Spawn! After That, Click On Spawn Pet! üêæ")
PetSpawnerTab:AddParagraph("Alt Account Warning! ‚ö†Ô∏è", "If You Are Using An Alt Account/Another Account Besdies Your Main, All Of The Exploits Will Not Work! This Will Only Work On Your Main Account Because The FetchData Is Stored On Your Main Account!")


PlayerTab:AddButton({
    Name = "Spawn Pet 🐾",
    Callback = function()
        local message = require(game.ReplicatedStorage.Library.Client.Message)
        message.Error("Spawning Selected Pet! Please Wait A Few Moments!")
        if petName then
            print("Spawning Pet:", petName)
            change("Cat", petName) -- Call the change function with the entered pet name
        else
            print("Please enter a pet name.")
        end
    end
})

-- Define the change function --
local lib = require(game.ReplicatedStorage.Library)

function change(fromPet, toPet)
    for i,v in pairs(lib.Directory.Pets[fromPet]) do
        lib.Directory.Pets[fromPet][i] = nil
    end
    for i,v in pairs(lib.Directory.Pets[toPet]) do
        lib.Directory.Pets[fromPet][i] = v
    end
end

